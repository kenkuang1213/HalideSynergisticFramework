<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Locallaplacian" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="process" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="$make" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile $target" />
					<SilentBuild command="$make &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Debug">
				<Option output="process" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<MakeCommands>
					<Build command="$make debug" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile $target" />
					<SilentBuild command="$make debug &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<ExtraCommands>
			<Add before="export HALIDE_PATH=$HOME/src/Halide" />
			<Add before="export LD_LIBRARY_PATH=$HOME/src/Halide/bin:$LD_LIBRARY_PATH" />
		</ExtraCommands>
		<Unit filename="CPULL.cpp">
			<Option target="Release" />
		</Unit>
		<Unit filename="CPULL.h">
			<Option target="Release" />
		</Unit>
		<Unit filename="JitInterface.h">
			<Option target="Release" />
		</Unit>
		<Unit filename="Syner.cpp">
			<Option target="Release" />
		</Unit>
		<Unit filename="Syner.h">
			<Option target="Release" />
		</Unit>
		<Unit filename="main.cpp">
			<Option target="Release" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
